{
  "contract_name": "axelar-router",
  "contract_version": "0.1.0",
  "idl_version": "1.0.0",
  "instantiate": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "InstantiateMsg",
    "type": "object",
    "additionalProperties": false
  },
  "execute": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "ExecuteMsg",
    "oneOf": [
      {
        "type": "object",
        "required": [
          "send_message_evm"
        ],
        "properties": {
          "send_message_evm": {
            "type": "object",
            "required": [
              "destination_address",
              "destination_chain",
              "message",
              "msg_type"
            ],
            "properties": {
              "destination_address": {
                "type": "string"
              },
              "destination_chain": {
                "type": "string"
              },
              "message": {
                "type": "string"
              },
              "msg_type": {
                "$ref": "#/definitions/MessageType"
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "send_message_cosmos"
        ],
        "properties": {
          "send_message_cosmos": {
            "type": "object",
            "required": [
              "destination_address",
              "destination_chain",
              "message",
              "msg_type"
            ],
            "properties": {
              "destination_address": {
                "type": "string"
              },
              "destination_chain": {
                "type": "string"
              },
              "message": {
                "type": "string"
              },
              "msg_type": {
                "$ref": "#/definitions/MessageType"
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "receive_message_cosmos"
        ],
        "properties": {
          "receive_message_cosmos": {
            "type": "object",
            "required": [
              "message",
              "sender"
            ],
            "properties": {
              "message": {
                "type": "string"
              },
              "sender": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "receive_message_evm"
        ],
        "properties": {
          "receive_message_evm": {
            "type": "object",
            "required": [
              "payload",
              "source_address",
              "source_chain"
            ],
            "properties": {
              "payload": {
                "$ref": "#/definitions/Binary"
              },
              "source_address": {
                "type": "string"
              },
              "source_chain": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "update_dest_config"
        ],
        "properties": {
          "update_dest_config": {
            "type": "object",
            "required": [
              "config"
            ],
            "properties": {
              "config": {
                "$ref": "#/definitions/DestConfig"
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "clear_stored_message"
        ],
        "properties": {
          "clear_stored_message": {
            "type": "object",
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "update_operators"
        ],
        "properties": {
          "update_operators": {
            "type": "object",
            "required": [
              "update_type"
            ],
            "properties": {
              "update_type": {
                "$ref": "#/definitions/UpdateType_for_Array_of_String"
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      }
    ],
    "definitions": {
      "Binary": {
        "description": "Binary is a wrapper around Vec<u8> to add base64 de/serialization with serde. It also adds some helper methods to help encode inline.\n\nThis is only needed as serde-json-{core,wasm} has a horrible encoding for Vec<u8>. See also <https://github.com/CosmWasm/cosmwasm/blob/main/docs/MESSAGE_TYPES.md>.",
        "type": "string"
      },
      "DestConfig": {
        "type": "object",
        "required": [
          "address",
          "chain"
        ],
        "properties": {
          "address": {
            "type": "string"
          },
          "chain": {
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "MessageType": {
        "type": "string",
        "enum": [
          "message",
          "message_with_token",
          "token"
        ]
      },
      "UpdateType_for_Array_of_String": {
        "oneOf": [
          {
            "type": "object",
            "required": [
              "add"
            ],
            "properties": {
              "add": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            },
            "additionalProperties": false
          },
          {
            "type": "object",
            "required": [
              "remove"
            ],
            "properties": {
              "remove": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            },
            "additionalProperties": false
          }
        ]
      }
    }
  },
  "query": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "QueryMsg",
    "oneOf": [
      {
        "type": "object",
        "required": [
          "get_stored_message"
        ],
        "properties": {
          "get_stored_message": {
            "type": "object",
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "get_ibc_response"
        ],
        "properties": {
          "get_ibc_response": {
            "type": "object",
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "get_dest_config"
        ],
        "properties": {
          "get_dest_config": {
            "type": "object",
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "get_operators"
        ],
        "properties": {
          "get_operators": {
            "type": "object",
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      }
    ]
  },
  "migrate": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "MigrateMsg",
    "type": "object",
    "additionalProperties": false
  },
  "sudo": null,
  "responses": {
    "get_dest_config": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "DestConfig",
      "type": "object",
      "required": [
        "address",
        "chain"
      ],
      "properties": {
        "address": {
          "type": "string"
        },
        "chain": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "get_i_b_c_response": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "IBCResponse",
      "type": "object",
      "required": [
        "ack",
        "channel",
        "registry",
        "sequence",
        "success"
      ],
      "properties": {
        "ack": {
          "type": "string"
        },
        "channel": {
          "type": "string"
        },
        "registry": {
          "type": "string"
        },
        "sequence": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "success": {
          "type": "boolean"
        }
      },
      "additionalProperties": false
    },
    "get_operators": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Array_of_String",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "get_stored_message": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Array_of_Message",
      "type": "array",
      "items": {
        "$ref": "#/definitions/Message"
      },
      "definitions": {
        "Message": {
          "type": "object",
          "required": [
            "message",
            "sender"
          ],
          "properties": {
            "message": {
              "type": "string"
            },
            "sender": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      }
    }
  }
}
